#1.
class Solution:
    def findContentChildren(self, g: List[int], s: List[int]) -> int:
        s.sort()
        g.sort()
        
        gi=0
        si=0
        result=0
        
        while gi<len(g) and si<len(s):
            if s[si]>=g[gi]:
                si += 1
                gi += 1
                result += 1
            else:
                si += 1
        return result
#2.
