#1.
class Solution(object):
    def isBoomerang(self, points):
        """
        :type points: List[List[int]]
        :rtype: bool
        """
        (x1,y1),(x2,y2),(x3,y3) = points
        if (x1*y2 + x2*y3 + x3*y1 - y1*x2-y2*x3-y3*x1) != 0:
            return True
        else:
            return False
