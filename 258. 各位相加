#1.
class Solution:
    def addDigits(self, num: int) -> int:
        if num <= 0:
            return 0
        else:
            result = (num - 1) % 9 + 1
            
        return result
#2.
class Solution:
    def addDigits(self, num: int) -> int:
        if num < 9:
            return num
        elif num % 9 == 0:
            return 9
        else:
            return num%9
